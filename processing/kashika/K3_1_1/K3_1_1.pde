size(600,400);
//属性の変更
background(255);//背景を白くする
strokeWeight(8);//線の太さを8にする
stroke(0,0,255);//線の色を青にする
fill(255,0,0); //内部を赤で塗りつぶす
//図形の描画
line(100,50,250,150); //線を描画する
rect(50,200,200,100);//矩形描画
ellipse(450,100,200,100);//楕円を描画する

fill(255,255,0);//内部を黄色で塗りつぶす
triangle(400,200,300,350,500,300);//三角形を描画
